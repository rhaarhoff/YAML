                            email:
                              type: string
                        bot:
                          type: object
                          properties:
                            owner:
                              type: object
                              properties:
                                type:
                                  type: string
                                workspace:
                                  type: boolean
                  next_cursor:
                    type: string
                    nullable: true
                  has_more:
                    type: boolean
                example:
                  object: list
                  results:
                    - object: user
                      id: 6794760a-1f15-45cd-9c65-0dfe42f5135a
