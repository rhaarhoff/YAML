  /v1/search:                                           
    post:
      tags:
        - Search
      summary: Search
      operationId: searchNotion
      requestBody:
        required: true
        content:
          application/json:
            schema:
              type: object
              properties:
                query:
                  type: string
                sort:
                  type: object
                  properties:
                    direction:
                      type: string
                      enum:
                        - ascending
                        - descending
                    timestamp:
                      type: string
              example:
                query: Media Article
                sort:
                  direction: ascending
                  timestamp: last_edited_time
      parameters:
        - name: Notion-Version
          in: header
          schema:
            type: string
          example: 2022-06-28
      responses:
        "200":
          description: Search results returned successfully
          content:
            application/json:
              schema:
                type: object
                properties:
                  object:
                    type: string
                    example: list
                  results:
                    type: array
                    items:
                      type: object
                      properties:
                        object:
                          type: string
                          example: page
                        id:
                          type: string
                          example: ae1905c3-b77b-475b-b98f-7596c242137f
                        created_time:
                          type: string
                          example: 2021-05-21T16:41:00.000Z
                        last_edited_time:
                          type: string
                          example: 2021-05-21T16:41:00.000Z
                        created_by:
                          type: object
                          properties:
                            object:
                              type: string
